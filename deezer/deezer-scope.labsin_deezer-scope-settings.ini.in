[sortMode]
type = list
defaultValue = 0
#Scope settings
_displayName = Sort Mode
_displayValues = Ranking;Rating;Track;Album;Artist;Duration

[sortOrder]
type = boolean
defaultValue = true
#Scope settings
_displayName = Sort Ascending (doesn't apply to Ranking)

[limitResults]
type = number
defaultValue = 20
#Scope settings
_displayName = Number of results to retrieve per query

[limitTracks]
type = number
defaultValue = 10
#Scope settings
_displayName = Number of top tracks to retrieve of an artist
